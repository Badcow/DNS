{
    "name": "badcow/dns",
    "description": "A PHP library for creating DNS zone files based on RFC1035",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Williams",
            "email": "sam@badcow.co"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4 || ^8.0",
        "rlanvin/php-ip": "^3.0",
        "christian-riesen/base32": "^1.5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.5.0",
        "phpstan/phpstan": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Badcow\\DNS\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Badcow\\DNS\\Tests\\": "tests/"
        }
    }
}
