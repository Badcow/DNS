{
    "name": "badcow/dns",
    "description": "A PHP library for creating DNS zone files based on RFC1035",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Williams",
            "email": "sam@badcow.co"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~7.2",
        "rlanvin/php-ip": "~2.0",
        "ademarre/binary-to-text-php": "~3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.4",
        "friendsofphp/php-cs-fixer": "~2.1",
        "phpstan/phpstan": "^0.11"
    },
    "autoload": {
        "psr-4": {
            "Badcow\\DNS\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Badcow\\DNS\\Tests\\": "tests/"
        }
    }
}
